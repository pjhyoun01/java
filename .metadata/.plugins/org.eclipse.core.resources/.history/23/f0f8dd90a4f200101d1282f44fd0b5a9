package java_exam;

public class IfExam {
	public static void main(String[] args) {
		int randomNum = (int)(Math.random() * 100);
		if (randomNum >= 10 && randomNum <20) {
			System.out.println("10 범위의 숫자가 나왔습니다");
			
		}	
		
//		int radix = randomNum / 10 * 10;
//		System.out.println(radix + "범위의 숫자가 나왔습니다");
	}
}
