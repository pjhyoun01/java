package exam;

public class Q10171 {
	public static void main(String[] args) {
		System.out.println("\"큰 따옴표 \" 안에서 \"큰 따옴표\"는 사용할 수 없습니다/");
		System.out.println("큰 따옴표 안에서는 백슬래시\\를 사용할 수 없습니다.");
		System.out.println("\\    /\\ ");
		System.out.println(" )  ( ')");
		System.out.println("(  /  )");
		System.out.println(" \\(__)|");
	}
}
