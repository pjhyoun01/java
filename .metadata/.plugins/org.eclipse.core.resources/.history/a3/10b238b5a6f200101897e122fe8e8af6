package exam.baekjoon;

import java.util.Scanner;

public class Q14681 {
	public static void main(String[] args) {
		int quadrant;

		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int y = sc.nextInt();

		if (x > 0 && y > 0) {
			quadrant = 1;
		} else if (x < 0 && y > 0) {
			quadrant = 2;
		} else if (x < 0 && y < 0) {
			quadrant = 3;
		} else if (x > 0 && y < 0) {
			quadrant = 4;
		} else if (x == 0 && y ==0) {
			System.out.println("");
		} else if (x == 0) {
			
		}
	}
}
