package java_exam;

public class Variables1 {

	public static void main(String[] args) {
//		중요한 타입 (int, long, float, double, boolean)
//		나이가 몇 살인지 구해보는 나이 계산기
//		(현재 연도) - (나의 생년)

		/*
		 * var Naming Rule - 첫 번째 단어의 첫 번째 글자는 소문자로 작성 
		 * - 두 번째 이후 단어의 첫 번째 들자만 대문자로 작성(Camel Case) 
		 * - 특수기호나 숫자로 시작 할 수 없다 
		 * 		사용 가능한 특수기호( - ) but 권장하지 않음
		 */
		{
			int nowYear;
			int myBornYear;
			int age;

			nowYear = 2026;
			myBornYear = 2001;
			age = nowYear - myBornYear + 1;

			System.out.println(age);
		}
		System.out.println("계산 끝났습니다.");
		
		long price = 1_000;
		long buildingPrice = 10_000_000_000L;
		
		float ratio = 5.11F;
		double doubleRatio = 5.1111111111111111111111D;
		
		final int spped = 1_000_000;
		
		
		
		
	}
}
